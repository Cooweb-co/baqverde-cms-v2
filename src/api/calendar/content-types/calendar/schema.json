{
  "kind": "collectionType",
  "collectionName": "calendars",
  "info": {
    "singularName": "calendar",
    "pluralName": "calendars",
    "displayName": "Calendar",
    "description": "Calendar with representative images for each month"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "month": {
      "type": "enumeration",
      "enum": [
        "Enero",
        "Febrero",
        "Marzo",
        "Abril",
        "Mayo",
        "Junio",
        "Julio",
        "Agosto",
        "Septiembre",
        "Octubre",
        "Noviembre",
        "Diciembre"
      ],
      "required": true,
      "unique": true
    },
    "monthNumber": {
      "type": "integer",
      "min": 1,
      "max": 12,
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
